{
  "name": "@elizaos-plugins/plugin-allora",
  "version": "0.1.9",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@alloralabs/allora-sdk": "^0.1.0",
    "@elizaos/core": "^1.0.0",
    "@elizaos/plugin-anthropic": "1.0.0",
    "@elizaos/plugin-bootstrap": "1.0.5",
    "node-cache": "^5.1.2",
    "vitest": "2.1.9",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "elizaos test",
    "lint": "prettier --write ./src",
    "lint:fix": "biome check --apply .",
    "format": "prettier --write ./src",
    "format:fix": "biome format --write .",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "ALLORA_CHAIN_SLUG": {
        "type": "string"
      },
      "ALLORA_API_KEY": {
        "type": "string"
      }
    }
  },
  "module": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/@elizaos-plugins/plugin-allora.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
